 ParseVariable←{
     ⍝ ⍺ ←→ Variable width format
     ⍝ ⍵ ←→ Character matrix of time strings
     ⍝ ← ←→ Time matrix
     m←' ',⍺ VariableMMM ⍵
     f←∪⍺∩'YMDhms'
     b←m∊⎕D
     r←+/2</b
     k←≢f
     j←r≠k
     n←+/j
     p←(⊃⌽⍴m)↑' 0'⍴⍨2×k
     m[⍸j;]←↑n⍴⊂p
     b[⍸j;]←1
     v←,m
     v[⍸~,b]←' '
     ⍉(⌽k,≢m)⍴1⊃⎕VFI v
 }
